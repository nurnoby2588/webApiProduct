{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "defaultConnection": "Server=(localdb)\\mssqllocaldb;Database=Demo_CURD;Trusted_Connection=True; TrustServerCertificate = True"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Seilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Informaton",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "MsSqlServer",
        "Args": {
          "connectionString": "DefaultConnection",
          "tableName": "ErrorLogs",
          "autoCreateSqlTable": true
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
